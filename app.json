{
  "expo": {
    "name": "Okidoki",
    "slug": "Okidoki",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/okidoki_icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/okidoki_splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "io.expo.client"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "io.expo.client"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "react-native-health",
        {
          "isClinicalDataEnabled": true,
          "healthSharePermission": "Custom health share permission",
          "healthUpdatePermission": "Custom health update permission"
        }
      ]
    ]
  }
}
